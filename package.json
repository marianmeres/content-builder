{
	"name": "@marianmeres/content-builder",
	"version": "1.33.0",
	"scripts": {
		"dev": "vite dev --force",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check .",
		"prettier": "prettier --write .",
		"release:patch": "release -v patch",
		"release": "release -v minor",
		"release:major": "release -v major"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^4.0.0"
	},
	"devDependencies": {
		"@marianmeres/release": "^1.1.2",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"autoprefixer": "^10.4.17",
		"postcss": "^8.4.35",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"publint": "^0.1.9",
		"sass": "^1.71.1",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.11"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"@marianmeres/clog": "^1.0.1",
		"@marianmeres/icons-fns": "^2.1.0",
		"@marianmeres/store": "^1.5.0",
		"@marianmeres/stuic": "^1.64.0",
		"@marianmeres/tree": "^1.0.2",
		"clsx": "^2.1.0",
		"esm-env": "^1.0.0",
		"tailwind-merge": "^2.2.1"
	}
}
